{
  "name": "brackette",
  "version": "0.2.1",
  "main": "./server/index.js",
  "pkg": {
    "assets": [
      "./client/build/**/*",
      "./client/build/**/*.map"
    ],
    "scripts": [
      "./client/build/**/*.js",
      "./node_modules/challonge/lib/api/*.js"
    ],
    "targets": [
      "node7"
    ]
  },
  "bin": {
    "brackette": "./server/index.js"
  },
  "description": "Run your challonge tournamnets with ease.",
  "scripts": {
    "start": "node ./server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon ./server/index.js",
    "client": "node ./server/tools/start-client.js",
    "lint": "eslint",
    "update": "node ./update.js"
  },
  "repository": {
    "url": "git+https://github.com/HappyZombies/brackette.git"
  },
  "author": "Daniel Reguero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HappyZombies/brackette/issues"
  },
  "homepage": "https://github.com/HappyZombies/brackette#readme",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "auto-updater": "^1.0.2",
    "body-parser": "^1.17.2",
    "challonge": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "opn": "^5.0.0",
    "promise": "^7.1.1",
    "socket.io": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "jest": "^20.0.4"
  }
}
