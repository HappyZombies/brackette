{
  "name": "brackette",
  "version": "0.2.1",
  "auto-updater":{
    "repo":"HappyZombies/brackette",
    "branch":"master"
  },
  "description": "Run your challonge tournamnets with ease.",
  "scripts": {
    "start": "node ./server/index.js",
