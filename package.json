  },
  "homepage": "https://github.com/HappyZombies/brackette#readme",
  "dependencies": {
    "auto-updater": "^1.0.2",
    "body-parser": "^1.17.2",
    "challonge": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "opn": "^5.0.0",
    "socket.io": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015-node": "^6.1.1",
